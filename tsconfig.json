{
    "compilerOptions": {
    "baseUrl": "src",
      "module": "commonjs",
      "target": "esnext",
      "jsx": "preserve",
      "lib": ["dom", "es2015", "es2017"],
      "strict": true,
      "noEmit": true,
      "isolatedModules": true,
      "esModuleInterop": true,
      "noUnusedLocals": false,
      "allowJs": true,
      "strictNullChecks": true,
    },
    "include": ["src"]
  }